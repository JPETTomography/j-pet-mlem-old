Cell[BoxData[
 RowBox[{
  RowBox[{"ReadPETMatrix", "[", "fileName_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"f", "=", 
       RowBox[{"OpenRead", "[", 
        RowBox[{"fileName", ",", 
         RowBox[{"BinaryFormat", "\[Rule]", "True"}]}], "]"}]}], ",", 
      "\[IndentingNewLine]", "type", ",", "tof", ",", "triangular", ",", 
      "output"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"type", "=", 
      RowBox[{"StringJoin", "@", 
       RowBox[{"BinaryReadList", "[", 
        RowBox[{"f", ",", "\"\<Character8\>\"", ",", "4"}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"ensure", " ", "we", " ", "have", " ", "proper", " ", "magic"}],
       " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"!", 
        RowBox[{"MatchQ", "[", 
         RowBox[{"type", ",", 
          RowBox[{
          "\"\<PETp\>\"", "|", "\"\<PETP\>\"", "|", "\"\<TOFp\>\"", "|", 
           "\"\<TOFP\>\""}]}], "]"}]}], ",", 
       RowBox[{"Return", "[", "Null", "]"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"tof", "=", 
      RowBox[{"StringMatchQ", "[", 
       RowBox[{"type", ",", 
        RowBox[{"\"\<TOF\>\"", "~~", "_"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"triangular", "=", 
      RowBox[{"StringMatchQ", "[", 
       RowBox[{"type", ",", 
        RowBox[{"___", "~~", "\"\<p\>\""}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"output", "=", 
      RowBox[{"{", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"\"\<Type\>\"", "\[Rule]", "type"}], ",", 
        RowBox[{"\"\<TOF\>\"", "\[Rule]", "tof"}], ",", 
        RowBox[{"\"\<Triangular\>\"", "\[Rule]", "triangular"}], ",", 
        RowBox[{"\"\<Full\>\"", "\[Rule]", 
         RowBox[{"!", "triangular"}]}], ",", "\[IndentingNewLine]", 
        RowBox[{"\"\<Pixels\>\"", "\[Rule]", 
         RowBox[{"BinaryRead", "[", 
          RowBox[{"f", ",", "\"\<UnsignedInteger32\>\""}], "]"}]}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"\"\<Emissions\>\"", "\[Rule]", 
         RowBox[{"BinaryRead", "[", 
          RowBox[{"f", ",", "\"\<UnsignedInteger32\>\""}], "]"}]}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"\"\<Detectors\>\"", "\[Rule]", 
         RowBox[{"BinaryRead", "[", 
          RowBox[{"f", ",", "\"\<UnsignedInteger32\>\""}], "]"}]}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"\"\<Positions\>\"", "\[Rule]", 
         RowBox[{"If", "[", 
          RowBox[{"tof", ",", 
           RowBox[{"BinaryRead", "[", 
            RowBox[{"f", ",", "\"\<UnsignedInteger32\>\""}], "]"}], ",", 
           "1"}], "]"}]}], ",", "\[IndentingNewLine]", 
        RowBox[{"\"\<Data\>\"", "\[Rule]", 
         RowBox[{"With", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"format", "=", 
             RowBox[{"If", "[", 
              RowBox[{"tof", ",", "\[IndentingNewLine]", 
               RowBox[{"(*", " ", 
                RowBox[{
                "TOF", " ", "has", " ", "position", " ", "information"}], " ",
                 "*)"}], "\[IndentingNewLine]", 
               RowBox[{"{", 
                RowBox[{
                "\"\<UnsignedInteger32\>\"", ",", "\"\<UnsignedInteger16\>\"",
                  ",", "\"\<UnsignedInteger16\>\"", ",", 
                 "\"\<UnsignedInteger32\>\""}], "}"}], ",", 
               "\[IndentingNewLine]", 
               RowBox[{"{", 
                RowBox[{
                "\"\<UnsignedInteger16\>\"", ",", "\"\<UnsignedInteger16\>\"",
                  ",", "\"\<UnsignedInteger32\>\""}], "}"}]}], "]"}]}], "}"}],
            ",", "\[IndentingNewLine]", 
           RowBox[{"First", "@", 
            RowBox[{"Last", "@", 
             RowBox[{"Reap", "@", 
              RowBox[{"While", "[", 
               RowBox[{"True", ",", "\[IndentingNewLine]", 
                RowBox[{"With", "[", 
                 RowBox[{
                  RowBox[{"{", "\[IndentingNewLine]", 
                   RowBox[{
                    RowBox[{"lor", "=", 
                    RowBox[{"BinaryReadList", "[", 
                    RowBox[{"f", ",", "\"\<UnsignedInteger16\>\"", ",", "2"}],
                     "]"}]}], ",", "\[IndentingNewLine]", 
                    RowBox[{"count", "=", 
                    RowBox[{"BinaryRead", "[", 
                    RowBox[{"f", ",", "\"\<UnsignedInteger32\>\""}], 
                    "]"}]}]}], "\[IndentingNewLine]", "}"}], ",", 
                  "\[IndentingNewLine]", 
                  RowBox[{
                   RowBox[{"If", "[", 
                    RowBox[{
                    RowBox[{"count", "\[Equal]", "EndOfFile"}], ",", 
                    RowBox[{"Break", "[", "]"}]}], "]"}], ";", 
                   "\[IndentingNewLine]", 
                   RowBox[{"(*", " ", 
                    RowBox[{
                    "converting", " ", "to", " ", "packed", " ", "makes", " ",
                     "it", " ", "more", " ", "memory", " ", "efficient"}], 
                    " ", "*)"}], "\[IndentingNewLine]", 
                   RowBox[{"Sow", "[", 
                    RowBox[{"lor", "\[Rule]", 
                    RowBox[{"Developer`ToPackedArray", "@", 
                    RowBox[{"BinaryReadList", "[", 
                    RowBox[{"f", ",", "format", ",", "count"}], "]"}]}]}], 
                    "]"}]}]}], "]"}]}], "]"}]}]}]}]}], "]"}]}]}], "}"}]}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"Close", "[", "f", "]"}], ";", "output"}]}], "]"}]}]], "Input"],

Cell[BoxData[
 RowBox[{
  RowBox[{"ImagePETMatrix", "[", 
   RowBox[{"m_", ",", "lors___"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"data", "=", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Length", "[", 
           RowBox[{"{", "lors", "}"}], "]"}], ">", "0"}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"FilterRules", "[", 
          RowBox[{
           RowBox[{"\"\<Data\>\"", "/.", "m"}], ",", 
           RowBox[{"Alternatives", "[", "lors", "]"}]}], "]"}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"\"\<Data\>\"", "/.", "\[VeryThinSpace]", "m"}]}], "]"}]}], 
      ",", "\[IndentingNewLine]", 
      RowBox[{"size", "=", 
       RowBox[{"\"\<Pixels\>\"", "/.", "\[VeryThinSpace]", "m"}]}], ",", 
      "total"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"SetSystemOptions", "[", 
      RowBox[{"\"\<SparseArrayOptions\>\"", "\[Rule]", 
       RowBox[{"{", 
        RowBox[{"\"\<TreatRepeatedEntries\>\"", "\[Rule]", "1"}], "}"}]}], 
      "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"total", "=", 
      RowBox[{"Total", "[", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"With", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"list", "=", 
              RowBox[{"Last", "[", "#", "]"}]}], "}"}], ",", 
            "\[IndentingNewLine]", 
            RowBox[{"Normal", "@", 
             RowBox[{"SparseArray", "[", 
              RowBox[{
               RowBox[{
                RowBox[{"(", 
                 RowBox[{
                  RowBox[{"(", 
                   RowBox[{"list", "\[LeftDoubleBracket]", 
                    RowBox[{"All", ",", 
                    RowBox[{"2", ";;", "3"}]}], "\[RightDoubleBracket]"}], 
                   ")"}], "+", "1"}], ")"}], "\[Rule]", 
                RowBox[{"list", "\[LeftDoubleBracket]", 
                 RowBox[{"All", ",", "4"}], "\[RightDoubleBracket]"}]}], ",", 
               
               RowBox[{"{", 
                RowBox[{"size", ",", "size"}], "}"}]}], "]"}]}]}], "]"}], 
          "&"}], "/@", "data"}], ",", "1"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"SetSystemOptions", "[", 
      RowBox[{"\"\<SparseArrayOptions\>\"", "\[Rule]", 
       RowBox[{"{", 
        RowBox[{"\"\<TreatRepeatedEntries\>\"", "\[Rule]", "0"}], "}"}]}], 
      "]"}], ";", "\[IndentingNewLine]", "total"}]}], "]"}]}]], "Input"],

Cell[BoxData[
 RowBox[{
  RowBox[{"FullPETMatrix", "[", "m_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "}"}], ",", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"\"\<Full\>\"", "/.", "\[VeryThinSpace]", "m"}], ",", "m"}], 
      "]"}], ";", "m"}]}], "]"}]}]], "Input"],

Cell[BoxData[
 RowBox[{
  RowBox[{"PlotPETMatrix", "[", 
   RowBox[{"m_", ",", "opts___"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "\[IndentingNewLine]", 
     RowBox[{"img", "=", 
      RowBox[{"ImagePETMatrix", "[", 
       RowBox[{"m", ",", 
        RowBox[{"Sequence", "@@", 
         RowBox[{"Cases", "[", 
          RowBox[{
           RowBox[{"{", "opts", "}"}], ",", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"\"\<LOR\>\"", "\[Rule]", "lor_"}], ")"}], "\[Rule]", 
            "lor"}]}], "]"}]}]}], "]"}]}], "\[IndentingNewLine]", "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"\"\<Triangular\>\"", "/.", "m"}], ",", 
       RowBox[{"img", "+=", 
        RowBox[{"Transpose", "[", "img", "]"}]}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"ArrayPlot", "[", 
      RowBox[{"img", ",", "\[IndentingNewLine]", 
       RowBox[{"DataReversed", "\[Rule]", "True"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"ImageSize", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"Large", ",", "Automatic"}], "}"}]}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"PlotLegends", "\[Rule]", "Automatic"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"FrameTicks", "\[Rule]", "Automatic"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"CoordinatesToolOptions", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"\"\<DisplayFunction\>\"", "\[Rule]", 
          RowBox[{"Function", "[", 
           RowBox[{"pt", ",", "\[IndentingNewLine]", 
            RowBox[{"pt", "/.", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{"x_", ",", "y_"}], "}"}], "\[RuleDelayed]", 
              RowBox[{"With", "[", 
               RowBox[{
                RowBox[{"{", 
                 RowBox[{"pos", "=", 
                  RowBox[{"Round", "@", 
                   RowBox[{"Clip", "[", 
                    RowBox[{
                    RowBox[{"{", 
                    RowBox[{"x", ",", "y"}], "}"}], ",", 
                    RowBox[{"{", 
                    RowBox[{"1", ",", "64"}], "}"}]}], "]"}]}]}], "}"}], ",", 
                
                RowBox[{"pos", "\[Rule]", 
                 RowBox[{"img", "\[LeftDoubleBracket]", 
                  RowBox[{"Sequence", "@@", "pos"}], 
                  "\[RightDoubleBracket]"}]}]}], "]"}]}]}]}], "]"}]}], 
         "}"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{"Sequence", "@@", 
        RowBox[{"DeleteCases", "[", 
         RowBox[{
          RowBox[{"{", "opts", "}"}], ",", 
          RowBox[{"\"\<LOR\>\"", "\[Rule]", "_"}]}], "]"}]}]}], "]"}]}]}], 
   "]"}]}]], "Input"]
}, Open  ]],

